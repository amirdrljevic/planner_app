<div class="container-fluid">
  <div class="row">
    <%= render "home/sidebar" %>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <% if notice %>
        <p class="notice"><%= notice %></p>
      <% end %>

      <% if alert %>
        <p class="alert"><%= alert %></p>
      <% end %>

      <%= yield %>


      <%= render 'home/footer'%>
    </main>
  </div>
</div>