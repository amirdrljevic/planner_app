<div id="<%= dom_id company %>">
  <p>
    <strong>Company name:</strong>
    <%= company.company_name %>
  </p>

  <p>
    <strong>Street address:</strong>
    <%= company.street_address %>
  </p>

  <p>
    <strong>Postal code:</strong>
    <%= company.postal_code %>
  </p>

  <p>
    <strong>City:</strong>
    <%= company.city %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= company.country %>
  </p>

</div>
